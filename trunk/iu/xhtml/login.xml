<pax:página título="Identificación" scriptPropio="sí" estiloPropio="sí"
		xmlns="http://www.w3.org/1999/xhtml"
		xmlns:pax="http://www.ilkebenson.com/pax"
		xmlns:ilkm="http://www.ilkebenson.com/ilkm">

	<h1>Identificación del Usuario</h1>
	<p>
		<form method="POST">
			<input type="hidden" id="uniqid"/>
			<fieldset id="fieldsetLogin">
				<div id="divLogin">
				<p class="etiqueta">
					Usuario:
					<input name="nombre" id="nombre" size="10"/>
				</p>
				<p class="etiqueta">
					Clave:&#160;&#160;&#160;
					<input type="password" name="clave" id="clave" size="10"/>
				</p>
				<input id="botonAcceder" type="button" value="Acceder"/>
				</div>
			</fieldset>
		</form>
	</p>
</pax:página>
